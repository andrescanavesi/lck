<script async="async" defer="true" charset="UTF-8" type="application/ld+json">
          {
          "@context":"http://schema.org",
          "@type": "Recipe",
          "name":"{{recipe.title}}",
          "description": "{{recipe.description}}",
          "image": "{{recipe.featured_image_url}}",
          "url": "{{recipe.linkToThisPage}}",
          "headline": "{{recipe.title}}",
          "recipeIngredient": {{pageRecipeIngredients}},
          "recipeInstructions": {{pageRecipeInstructions}},
          "recipeCategory": "{{pageRecipeCategory}}",
          "prepTime":"{{pageRecipePrepTime}}",
          "cookTime":"{{pageRecipeCookTime}}",
          "totalTime":"{{pageRecipeTotalTime}}",
          "recipeCuisine":"{{pageRecipeCusine}}",
          "recipeYield":"{{pageRecipeYield}}",
          "video":"{{pageRecipeVideo}}",
          "aggregateRating": {
              "@type": "AggregateRating",
              "ratingValue": "{{aggregateRating}}",
              "ratingCount": "{{ratingCount}}"
          },
          "dateCreated": "{{recipe.created_at_friendly_2}}",
          "datePublished": "{{recipe.created_at_friendly_2}}",
          "dateModified": "{{recipe.updated_at_friendly_2}}",
          "inLanguage": "es-ES",
          "author": {
            "@type": "Person",
            "name": "Martín Kesman",
            "url": "https://www.lacocinadekesman.com"
          },
          "mainEntityOfPage": "True",
          "keywords": "{{recipe.tags_csv}}",
          "articleBody": "{{recipe.description}}",
          "publisher": {
            "@type": "Organization",
            "name": "La Cocina de Kesman"
           }
        }

</script>



<div class="section metro_post-single">
	<div class="container">
		<div class="row">
			<div class="col-lg-8">
				<!-- Content  -->
				<div class="metro_post-single-wrapper metro_recipe-single-wrapper">

					<h2 class="entry-title">{{recipe.title}}</h2>

					<div class="d-flex justify-content-between">
						<div class="metro_rating-wrapper">
							<div class="metro_rating">
								<img class="lozad" data-src="/images/icons/png/star.png" alt="star" width="16" />
								<img class="lozad" data-src="/images/icons/png/star.png" alt="star" width="16"/>
								<img class="lozad" data-src="/images/icons/png/star.png" alt="star" width="16"/>
								<img class="lozad" data-src="/images/icons/png/star.png" alt="star" width="16"/>
								<img class="lozad" data-src="/images/icons/png/star.png" alt="star" width="16"/>
							</div>
							<span>5 Estrellas</span>


						</div>

						<!--social icons for sharing-->
						<div class="ml-1 mb-2">
							<a class="ml-2" id="whastappShare" data-pin-do="buttonWhatsapp"
							   href="{{recipe.whatsappSharingUrl}}"
							   onclick="trackEvent(this,'social_whatsapp')">
								<img class="share-icon"
								     src="data:image/svg+xml;base64,">
							</a>
							<a class="ml-2" id="pinterestShare" data-pin-do="buttonPin"
							   href="{{recipe.pinterestSharingUrl}}"
							   onclick="trackEvent(this,'social_pinterest')">
								<img class="share-icon" src="data:image/svg+xml;base64,">
							</a>
							<a class="ml-2" id="facebookShare" data-pin-do="buttonFacebook"
							   href="{{recipe.facebookSharingUrl}}"
							   onclick="trackEvent(this,'social_facebook')">
								<img class="share-icon" src="data:image/svg+xml;base64,">
							</a>

							<a class="ml-2" id="twitterShare" data-pin-do="buttonTwitter"
							   href="{{recipe.twitterSharingUrl}}"
							   onclick="trackEvent(this,'social_twitter')">
								<img class="share-icon" src="data:image/svg+xml;base64,">
							</a>

						</div>


					</div>

					<div class="metro_post-single-thumb">
						<img class="lozad" data-src="{{recipe.featured_image_url}}" alt="foto receta">
						<div class="metro_post-date d-none">
							<span>25</span>
							<span>Dic</span>
						</div>
					</div>

					<!-- Entry Content Start -->
					<div class="entry-content">
						<span class="metro_post-meta">
							<i class="far fa-user"></i> By Martín
							<i class="far fa-clock"></i> {{recipe.total_time}}
							<span class="d-none"><a href="#"> <i class="far fa-clock"></i> Avanzado</a></span>
						</span>

						<p>{{recipe.description}}</p>

						<div class="row">
							<div class="col-lg-6">
								<div class="metro_ingredients">
									<h4>Ingredientes</h4>
									<ul class="ingredients-list">
										{{#each recipe.ingredients_array}}
											<li>{{this}} </li>
										{{/each}}
									</ul>
									{{#if recipe.has_extra_ingredients}}
										<h5 class="mt-3">{{recipe.extra_ingredients_title}}</h5>
										<ul class="ingredients-list">
											{{#each recipe.extra_ingredients_array}}
												<li>{{this}}</li>
											{{/each}}
										</ul>
									{{/if}}

								</div>
							</div>
							<div class="col-lg-6">
								<div class="metro_ingredients">
									<h4>Elaboración</h4>
									<ul class="steps-list">
										{{#each recipe.steps_array}}
											<li>{{this}} </li>
										{{/each}}
									</ul>
								</div>
							</div>
						</div>

						<p class="mb-3">{{recipe.notes}}</p>

						<div class="row">
							{{#each recipe.images_urls}}
								<div class="col-sm-6">
									<img class="lozad w-100" data-src="{{this}}" alt="Recipe">
								</div>
							{{/each}}
						</div>



						{{#if recipe.has_youtube_video}}
						<div class="embed-responsive embed-responsive-16by9">
							<iframe class="embed-responsive-item"
							        src="https://www.youtube.com/embed/{{recipe.youtube_video_id}}?rel=0"
							        frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
							        allowfullscreen>

							</iframe>
						</div>
						{{/if}}

					</div>
					<!-- Entry Content End -->

				</div>

				<!-- Related Posts -->
				<div class="metro_related-posts">

					<h4>Más recetas</h4>

					<div class="metro_related-posts-slider">

						<div class="container">
							<div class="row">

						{{#each relatedRecipes}}
							<div class="col-lg-6 col-md-6">
								<!-- Recipe Start -->
								<article class="metro_post metro_recipe">
									<div class="metro_post-thumb">
										<a href="{{this.url}}">
											<img class="lozad" data-src="{{this.featured_image_url}}" alt="foto receta">
										</a>
									</div>
									<div class="metro_post-body">
										<div class="metro_post-desc">
											<span class="metro_post-meta"> <a href="{{this.url}}">
												<i class="far fa-clock"></i> {{this.total_time}} </a>
												<a href="{{this.url}}" class="d-none">
													<i class="far fa-knife-kitchen"></i> Avanzado</a>
											</span>
											<h5> <a href="{{this.url}}">
												{{this.title}}
											</a> </h5>
											<p>{{this.description}}</p>
										</div>
										<a class="btn-link" href="{{this.url}}"> Leer Más </a>
									</div>
								</article>
								<!-- Recipe End -->
							</div>
						{{/each}}
							</div>
						</div>
					</div>

				</div>



			</div>

			<!-- Sidebar Start -->
			<div class="col-lg-4">
				<div class="sidebar">

					<!-- About author Start-->
					<div class="sidebar-widget widget-about-author">
						<div class="widget-about-author-inner">
							<img class="lozad" data-src="/images/lck-logo.png" alt="author">
							<h5>Martín Kesman</h5>
							<p>Praesent sapien massa, convallis a pellentesque nec, egestas non nisi. Donec sollicitudin molestie malesuada. Mauris blandit aliquet elit</p>
						</div>
						<ul class="metro_sm">
							<li><a href="https://www.instagram.com/lacocinadekesman/" class="instagram1">
								<img class="lozad" data-src="/images/icons/png/029-instagram.png" width="32"/>
							</a></li>
							<li><a href="https://www.facebook.com/La-Cocina-De-Kesman-1628880720697771/" class="facebook1">
								<img class="lozad" data-src="/images/icons/png/036-facebook.png" width="32"/>
							</a></li>
							<li><a href="https://www.youtube.com/channel/UCWs6GwAuccOhLrryaAZd2Xw" class="youtube1">
								<img class="lozad" data-src="/images/icons/png/001-youtube.png" width="32"/>
							</a></li>
							<li><a href="https://twitter.com/mkesman" class="twitter1">
								<img class="lozad" data-src="/images/icons/png/008-twitter.png" width="32"/>
							</a></li>
						</ul>
					</div>
					<!-- About author End-->

					<!-- Recent Posts Start -->
					<div class="sidebar-widget widget-recent-posts">
						<h5 class="widget-title">Recetas recientes</h5>
						{{#each latestRecipes}}
						<article class="post">
							<a href="{{this.url}}">
								<img class="lozad" data-src="{{this.featured_image_url}}" alt="receta"></a>
							<div class="post-content">
								<a href="{{this.url}}"> {{this.title}} </a>
								<h6> {{this.total_time}} </h6>
							</div>
						</article>
						{{/each}}

					</div>
					<!-- Recent Posts End -->

					<!-- Categories Start -->
					<div class="sidebar-widget">
						<h5 class="widget-title"> Recetas por Categoría </h5>
						<ul class="sidebar-widget-list">
							<li> <a href="#"> Veganas </a> </li>
							<li> <a href="#"> Desaynos </a> </li>
							<li> <a href="#"> Pizzas </a> </li>
							<li> <a href="#"> Ensaladas</a> </li>
							<li> <a href="#"> Tragas  </a> </li>
							<li> <a href="#"> Panes </a> </li>
							<li> <a href="#"> Almuerzos </a> </li>
							<li> <a href="#"> Cenas </a> </li>
						</ul>
					</div>
					<!-- Categories End -->

					<!-- Popular Tags Start -->
					<div class="sidebar-widget">
						<h5 class="widget-title">Tags más Vistas</h5>
						<div class="tagcloud">
							<a href="#">Cookies</a>
							<a href="#">Muffins</a>
							<a href="#">Fácil</a>
							<a href="#">Organico</a>
							<a href="#">Pescados</a>
							<a href="#">Green</a>
							<a href="#">Fibra</a>
							<a href="#">Rápida</a>
							<a href="#">Sano</a>
						</div>
					</div>
					<!-- Popular Tags End -->

				</div>
			</div>
			<!-- Sidebar End -->

		</div>

	</div>
</div>
